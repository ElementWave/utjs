{
  "name": "utjs",
  "version": "1.5.0",
  "description": "utjs is a high performance library with some common functions like sort, concatArrays, randomString, stringToNumber, etc.",
  "main": "index.js",
  "files": ["index.js", "lib"],
  "scripts": {
    "start": "node index.js",
    "test": "mocha test/*.js",
    "jshint": "jshint index.js lib/*.js test/*.js",
    "jscs": "jscs index.js lib/*.js test/*.js",
    "jscs:fix": "jscs --fix index.js lib/*.js test/*.js",
    "check": "npm run test && npm run jshint && npm run jscs",
    "doc": "rm -rf ./doc && jsdoc ./lib/ ./package.json ./README.md -d ./doc"
  },
  "author": "Alejandro Santiago Nieto",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.2.0",
    "jscs": "^2.0.0",
    "jsdoc": "^3.4.0",
    "jshint": "^2.8.0",
    "mocha": "^2.2.5"
  }
}
